<?xml version="1.0" encoding="UTF-8"?>
<project name="product" default="compile.test">
    <include file="../build/shared_build.xml"/>

    <target name="compile.source">
        <compile-module-source module="product"/>
    </target>

    <target name="compile.test">
        <compile-module-test module="product"/>
    </target>

    <target name="compile" depends="compile.source, compile.test"/>

    <target name="clean">
        <clean-module module="product"/>
    </target>
</project>